@model IEnumerable<WEB_SITE.Models.Escalas>

@{
    ViewData["Title"] = "Listado de Escalas";
}

<h1>@ViewData["Title"]</h1>

<p>
    <input type="submit" value="Create" class="btn btn-primary" />
</p>
<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.DuracionEscala)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DuracionLlegada)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.DuracionEscala)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DuracionLlegada)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                    </td>
                </tr>
            }
        }
        else
        {
            <h1>No hay data</h1>
        }
    </tbody>
</table>
